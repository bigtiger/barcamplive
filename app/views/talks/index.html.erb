<div style="clear:both;height:20px"></div>

<table id="talks">
  <tr>
    <th>Time</th>
    <th>Speaker</th>
    <th>Topic</th>
    <th></th>
    <th></th>
  </tr>

  <% for talk in @talks %>
    <tr>
      <td><%=h talk.starts_at.to_s(:time) %></td>
      <td><%=h talk.speaker %></td>
      <td><%=h talk.topic %></td>
      <td><%= link_to 'Edit', edit_venue_talk_path(@venue, talk) %></td>
    </tr>
  <% end %>
</table>
