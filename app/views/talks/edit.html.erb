<h2>Editing Talk for  <%= @venue.name %> at <%= @talk.starts_at.to_s(:time) %></h2>

<%= error_messages_for :talk %>

<% form_for([@venue, @talk]) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>

  <p>
    <%= f.submit "Update" %>
    or
    <%= link_to 'Cancel', venue_talks_path(@venue) %>
  </p>
<% end %>
